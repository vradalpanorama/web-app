    "accommodation": [
        {
            "slug": "vradal-booking",
            "preview": {
                "title": "Vrådal Booking",
                "lead": "Here you can rent large cabins and apartments in beautiful Norwegian mountain scenery. Most of the cabins and apartments are within walking distance of Vrådal Panorama Ski Center and several of them offer skiing in/out to the slopes.",
                "link": "http://www.vraadalbooking.no/"
            }
        },
        {
            "slug": "straand-hotell",
            "preview": {
                "title": "Straand Hotel",
                "lead": "One of Telemark's premier full-service hotels. Straand Hotel in Vrådal oozes history, unique facilities, traditional cuisine and an atmosphere you will be looking for for a long time.",
                "link": "http://straand.no/"
            }
        },
        {
            "slug": "vradal-hyttegrend",
            "preview": {
                "title": "Vrådal Hyttegrend",
                "lead": "Vrådal Hyttegrend has 16 rental cabins of a high standard. The cabins are designed for two families and are fully equipped for 8 people, with shower, wc, sauna, dishwasher, wood-burning stove and cable TV. Sunny location with great views and child-friendly surroundings.",
                "link": "http://www.hyttegrend.no/"
            }
        },
        {
            "slug": "fiskebekk",
            "preview": {
                "title": "Fiskebekk",
                "lead": "Fiskebekk offers rental of both small and large cabins with free wireless internet. The cabins have a high standard with a dishwasher and sauna, and are situated in a natural setting with a nice view of the lake Vråvatn. Distance to Vrådal Panorama Ski Center is 1.5 - 2.5 km.",
                "link": "http://www.fiskebekk.net/"
            }
        },
        {
            "slug": "nedre-strand",
            "preview": {
                "title": "Nedre Strand",
                "lead": "Nedre Strand cabin rental and camping is conveniently located in the center of Vrådal and borders the lake Nisser. The cabins are located in open south-facing terrain among pine trees, and with a nice view over the water and Vrådal's idyllic nature with the surrounding mountains.",
                "link": "http://www.nedrestrand.no/"
            }
        },
        {
            "slug": "nisser-hyttesenter",
            "preview": {
                "title": "Nisser Hyttesenter",
                "lead": "The cabin center consists of five modern cabins located 20-60 meters from the beautiful lake Nisser. As a guest, you will be kindly received by the hosts Tove and Ketil Øy. You can easily get out into the terrain where you can see moose and beaver, go on walks or enjoy nature.",
                "link": "http://hyttesenter.no/"
            }
        }
    ],
